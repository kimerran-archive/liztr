<div>
    <div ng-init="loadAllEvents()"></div>

    <table class="table table-hover">
        <tr>
            <th>Event</th>
            <th>&nbsp;</th>
            <th>&nbsp;</th>
            <th>&nbsp;</th>
        </tr>
        <tr ng-repeat="e in ownedEvents">
            <td>{{e.Name}}</td>
            <td>
                <a href="#/events/{{e.Id}}/reg">
                    Register
                </a>
            </td>
            <td>
                <a href="#/events/{{e.Id}}/pre">
                    Pre-Register
                </a>
            </td>
            <td>
                <a href="#/events/{{e.Id}}/edit">
                    Edit
                </a>
            </td>
        </tr>
    </table>
</div>
